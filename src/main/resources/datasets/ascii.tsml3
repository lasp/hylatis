<?xml version="1.0" encoding="UTF-8"?>

<dataset id="ascii">

  <adapter class="latis.reader.adapter.AsciiAdapter3"
           delimiter="\s+"
           commentCharacter="#"
           location="datasets/mixed.txt" />

  <time id="time" type="text" length="10">
    <metadata name="myTime" units="yyyy/MM/dd"/>
  </time>
 
  <integer id="myInt"/>
  <real    id="myReal"/>
  <text    id="myText" length="1"/>
  
  <!--?select time>1970-01-01?-->
  <!--?project myTime?-->
  <!--?format_time yyyy-MM-dd?-->

</dataset>
